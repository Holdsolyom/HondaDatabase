
CIwUIElement
{
	name "Item"
	CIwUILabel
	{
		name "ID"
		visible false
		style <font_name>
	}
	CIWUIImage
	{
		name "Icon"
		style <Icon>
	}
	CIwUIButton
	{
		name "Button"
		style <Button>
		CIwUIElement
		{
			CIwUILabel
			{
				name "Name"
				style <Name>
			}
			CIwUILabel
			{
				name "Desc"
				style <Desc>
			}
			CIwUILayoutVertical
			{	
				element
				{
					name "Name"
					alignV "top"
				}
				element
				{
					name "Desc"
					alignV "bottom"
				}
			}
		}
	}
	CIwUILayoutHorizontal
	{
		element
		{
			name "Icon"
			alignH "left"
			alignV "middle"
		}
		element
		{
			name "Button"
			alignH "right"
			alignV "middle"
		}
	}
}